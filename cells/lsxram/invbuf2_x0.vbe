ENTITY invbuf2_x0 IS
PORT(
  ax	: IN BIT;
  ay	: IN BIT;
  rx0	: INOUT BIT;
  rx1	: OUT BIT;
  ry0	: INOUT BIT;
  ry1	: OUT BIT;
  vdd	: IN BIT;
  vss	: IN BIT
);
END invbuf2_x0;

ARCHITECTURE VBE OF invbuf2_x0 IS

BEGIN

  rx0 <= NOT(ax);
  rx1 <= NOT(rx0);
  ry0 <= NOT(ay);
  ry1 <= NOT(ry0);

END VBE;
